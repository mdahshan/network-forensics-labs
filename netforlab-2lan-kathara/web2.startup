ip link set eth0 up
ip address add 192.168.5.12/24 dev eth0

ip route add 192.168.0.0/24 via 192.168.5.1
ip route add 10.0.0.0/8 via 192.168.5.1
ip route add default via 192.168.5.1

printf "search netforlab.net\nnameserver 192.168.0.10\n" > /etc/resolv.conf

chmod -R a+rX /usr/share/nginx/html

nginx -g "daemon off;" &
